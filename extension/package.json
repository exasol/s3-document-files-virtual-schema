{
  "name": "s3-document-files-virtual-schema-extension",
  "version": "0.1.0",
  "main": "dist/extension.js",
  "description": "Extension for s3 virtual schemas",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "npm run generate && npm run compile",
    "compile": "tsc --build && esbuild dist/extension.js --bundle --outfile=dist/s3-vs-extension.js --target=es6",
    "generate": "bash generate-config.sh",
    "lint": "eslint ./src/",
    "clean": "rm -rf dist/",
    "test": "jest --silent",
    "test-watch": "jest --watch"
  },
  "dependencies": {
    "@exasol/extension-manager-interface": "0.4.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "esbuild": "^0.20.2",
    "eslint": "^8.57.0",
    "jest": "29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0"
  }
}
